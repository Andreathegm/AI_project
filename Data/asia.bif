  type discrete [ 2 ] { Yes, No };
}
variable xray {
  type discrete [ 2 ] { Yes, No };
}
variable dysp {
  type discrete [ 2 ] { Yes, No };
}
probability ( asia ) {
  table 0.01, 0.99;
}
probability ( tub | asia ) {
  (Yes) 0.05, 0.95;
  (No) 0.01, 0.99;
}
probability ( smoke ) {
  table 0.5, 0.5;
}
