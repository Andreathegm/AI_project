network unknown {
}
variable asia {
  type discrete [ 2 ] { Yes, No };
}
variable tub {
  type discrete [ 2 ] { Yes, No };
}
variable smoke {
  type discrete [ 2 ] { Yes, No };
}
variable lung {
  type discrete [ 2 ] { Yes, No };
}
variable bronc {
  type discrete [ 2 ] { Yes, No };
}
