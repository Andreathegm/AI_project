  (No, Yes) 1.0, 0.0;
  (Yes, No) 1.0, 0.0;
  (No, No) 0.0, 1.0;
}
probability ( xray | either ) {
  (Yes) 0.98, 0.02;
  (No) 0.05, 0.95;
}
probability ( dysp | bronc, either ) {
  (Yes, Yes) 0.9, 0.1;
